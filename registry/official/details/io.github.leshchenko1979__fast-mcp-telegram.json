{
  "id": "io.github.leshchenko1979/fast-mcp-telegram",
  "displayName": "io.github.leshchenko1979/fast-mcp-telegram",
  "description": "Telegram MCP server with search and messaging capabilities",
  "version": "0.5.0",
  "status": "active",
  "publishedAt": "2025-09-17T14:08:48.770809Z",
  "updatedAt": "2025-09-17T14:08:48.770809Z",
  "iconUrl": null,
  "websiteUrl": null,
  "repository": {
    "url": "https://github.com/leshchenko1979/fast-mcp-telegram",
    "source": "github"
  },
  "packages": [
    {
      "registryType": "pypi",
      "identifier": "fast-mcp-telegram",
      "version": "0.5.0",
      "transport": {
        "type": "stdio"
      },
      "environmentVariables": [
        {
          "name": "API_ID",
          "description": "Telegram API ID (from https://my.telegram.org/apps)",
          "isRequired": true,
          "isSecret": false
        },
        {
          "name": "API_HASH",
          "description": "Telegram API Hash (from https://my.telegram.org/apps)",
          "isRequired": true,
          "isSecret": true
        },
        {
          "name": "SERVER_MODE",
          "description": "Server mode: stdio (local), http-no-auth (dev), http-auth (prod)",
          "isRequired": false,
          "isSecret": false,
          "default": "stdio",
          "choices": [
            "stdio",
            "http-no-auth",
            "http-auth"
          ]
        },
        {
          "name": "SESSION_DIR",
          "description": "Custom session directory (defaults to ~/.config/fast-mcp-telegram/)",
          "isRequired": false,
          "isSecret": false
        }
      ],
      "packageArguments": [],
      "runtimeArguments": []
    }
  ],
  "remotes": []
}